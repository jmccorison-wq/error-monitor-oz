# Warp Oz Configuration
WARP_API_KEY=your-warp-api-key
OZ_ENVIRONMENT_ID=your-oz-environment-id

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
DYNAMODB_TABLE_NAME=audit-log

# Azure DevOps Configuration
AZURE_DEVOPS_ORG=your-organization
AZURE_DEVOPS_PROJECT=your-project
AZURE_DEVOPS_PAT=your-personal-access-token

# GitHub Configuration
GITHUB_TOKEN=your-github-token
GITHUB_ORG=your-github-org

# Microsoft Teams Configuration
TEAMS_ACCESS_TOKEN=your-teams-access-token
TEAMS_TEAM_ID=your-team-id
TEAMS_CHANNEL_ID=your-channel-id

# Redash Configuration
REDASH_URL=https://your-redash-instance.com
REDASH_API_KEY=your-redash-api-key

# Application Settings
USE_MOCK_ADAPTERS=false
POLL_INTERVAL_MS=60000
MAX_ERRORS_PER_CYCLE=10
LOG_LEVEL=info
NODE_ENV=production